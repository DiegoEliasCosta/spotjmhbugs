<?xml version="1.0" encoding="UTF-8"?>
<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="findbugsplugin.xsd"
	pluginid="de.heidelberg.pvs.diego.performance-tests-checker">

	<Detector class="de.heidelberg.pvs.diego.checkers.MyDetector" reports="MY_BUG"
		speed="fast" />
	<!-- <Detector class="de.heidelberg.pvs.diego.VoidBenchmarkDetector"
		reports="JMH_VOID_BENCHMARK" speed="fast" /> -->

	<Detector class="de.heidelberg.pvs.diego.checkers.IgnoredMethodReturnChecker"
		reports="JMH_IGNORED_METHOD_RETURN" speed="fast" />

	<BugPattern type="MY_BUG" category="CORRECTNESS" />

	<BugPattern type="JMH_IGNORED_METHOD_RETURN" category="CORRECTNESS" />
</FindbugsPlugin>
